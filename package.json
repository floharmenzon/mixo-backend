        "express": "^4.21.2",
        "node-fetch": "^3.3.2",
        "nodemailer": "^6.10.1",
        "pdfkit": "^0.13.0",
        "qrcode": "^1.5.4"
    },
    "devDependencies": {
        "nodemon": "^2.0.22",
        "prisma": "^5.22.0"
    }
}
